    	<!-- ================================== SCP RCF Helmet ====================================-->
  
      <ThingDef ParentName="ArmorHelmetMakeableBase">
    <defName>Apparel_ScpRcfHelmet</defName>
    <label>RCF Helmet (SCP) </label>
    <description>A Helmet used by the Foundations' RCF Opperatives, it has a night vision mount and tactical glasses.</description>
    <techLevel>Industrial</techLevel>
	
    <recipeMaker>
      <researchPrerequisite>Smithing</researchPrerequisite>
	  <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>4</Crafting>
      </skillRequirements>
    </recipeMaker>
	
    <graphicData>
      <texPath>Things/Apparel/Display/SCP/SCP_RCFHelmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
      <WorkToMake>8000</WorkToMake>
      <MaxHitPoints>120</MaxHitPoints>
      <Mass>1.5</Mass>
      <StuffEffectMultiplierArmor>0.65</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.20</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2.5</EquipDelay>
    </statBases>
	
		<equippedStatOffsets>
		    <ShootingAccuracyPawn>0.06</ShootingAccuracyPawn>
			<PsychicSensitivity>-0.50</PsychicSensitivity>
			<GlobalLearningFactor>0.25</GlobalLearningFactor>
		</equippedStatOffsets>
			
	
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>40</costStuffCount>
    <costList>
      <!--<ComponentIndustrial>2</ComponentIndustrial>-->
			<Steel>10</Steel>
    </costList>
    <apparel>
	<careIfWornByCorpse>false</careIfWornByCorpse>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Hats/SCP/SCP_RCFHelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>SCP_RCFHeadgear</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
  
  
