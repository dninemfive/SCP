<Defs>
 <!-- ======== SCP FOUNDATION=========-->
  
  
           <!-- ======================================= SCP Facility Security Armor =======================================-->
  
 <ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_ScpSecurityArmor</defName>
    <label>Security Armor (SCP) </label>
    <description>A light Armored Vest used by Facility Security Forces.</description>
    <techLevel>Industrial</techLevel>
    <recipeMaker>
      <researchPrerequisite>FlakArmor</researchPrerequisite>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Apparel/Display/SCP/SCP_FacilitySecurityArmor</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
      <drawSize>0.90</drawSize>
    </graphicData>
    <statBases>
      <WorkToMake>8500</WorkToMake>
      <MaxHitPoints>200</MaxHitPoints>
      <Mass>3.0</Mass>
      <Flammability>0.6</Flammability>
      <ArmorRating_Sharp>.99</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.15</ArmorRating_Blunt>
      <ArmorRating_Heat>0.17</ArmorRating_Heat>
      <Insulation_Cold>1</Insulation_Cold>
      <EquipDelay>3.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.03</MoveSpeed>
	  <AimingDelayFactor>-0.11</AimingDelayFactor>
    </equippedStatOffsets>
    <thingCategories>
      <li>SCP_Armor</li>
    </thingCategories>
    <costList>
      <Cloth>20</Cloth>
      <Steel>45</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <apparel>
	<careIfWornByCorpse>false</careIfWornByCorpse>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Armor/SCP/SCP_FacilitySecurityArmor</wornGraphicPath>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
	    <li>SCP_SecurityUniforms</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
  
  
