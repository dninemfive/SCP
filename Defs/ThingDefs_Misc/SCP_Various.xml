<Defs>
 <!-- ======== SCP FOUNDATION=========-->
 
 
 
 <!-- ==================================== SCP D Class Uniform  =====================================-->
 
 <ThingDef ParentName="ApparelSCPBase">
    <defName>Apparel_ScpDClassUniform</defName>
    <label>D-Class Uniform (SCP) </label>
    <description>An orange jumpsuit worn by D Class inmates within a SCP Facility.</description>
    <graphicData>
      <texPath>Things/Apparel/Display/SCP/SCP_DClass</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <costStuffCount>75</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <thingCategories>
      <li>SCP_Clothing</li>
    </thingCategories>
    <statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<WorkToMake>1500</WorkToMake>
      <Mass>0.20</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.35</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.15</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.6</EquipDelay>
    </statBases>
	<equippedStatOffsets>
	  <WorkSpeedGlobal>0.25</WorkSpeedGlobal>
	  <CarryingCapacity>10</CarryingCapacity>
	  <SocialImpact>-0.25</SocialImpact>
    </equippedStatOffsets>
	
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
		<li>Arms</li>
		<li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Clothing/SCP/SCP_DClass</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>SCP_DClass</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
  
